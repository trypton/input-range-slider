{
  "version": 2,
  "tags": [
    {
      "name": "input-range-slider",
      "description": "An input range webcomponent with interval support",
      "properties": [
        {
          "name": "value",
          "type": "Number|[Number, Number]",
          "description": "Input value, either integer number or an array that represents an interval",
          "default": "50"
        },
        {
          "name": "min",
          "type": "Number",
          "description": "The minimum permitted value",
          "default": 0
        },
        {
          "name": "max",
          "type": "Number",
          "description": "The maximum permitted value",
          "default": 0
        },
        {
          "name": "step",
          "type": "Number",
          "description": "The stepping interval, used both for user interface and validation purposes",
          "default": 0
        }
      ],
      "events": [],
      "slots": [],
      "cssProperties": [
        {
          "name": "--input-range-slider-color-primary",
          "description": "Main Color",
          "default": "#3f51b5"
        },
        {
          "name": "--input-range-slider-color-track",
          "description": "Track Color",
          "default": "#eee"
        },
        {
          "name": "--input-range-slider-color-fill",
          "description": "Track Fill Color",
          "default": "var(--input-range-slider-color-primary)"
        },
        {
          "name": "--input-range-slider-color-thumb",
          "description": "Thumb Color",
          "default": "var(--input-range-slider-color-primary)"
        },
        {
          "name": "--input-range-slider-color-outline",
          "description": "Outline Color",
          "default": "rgb(159 168 218 / 50%)"
        },
        {
          "name": "--input-range-slider-track-height",
          "description": "Track Height",
          "default": "4px"
        },
        {
          "name": "--input-range-slider-thumb-size",
          "description": "Thumb Size",
          "default": "18px"
        }
      ]
    }
  ]
}
