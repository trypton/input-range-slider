{
  "version": 2,
  "tags": [
    {
      "name": "input-range-slider",
      "description": "An input range component with min and max values support",
      "properties": [
        {
          "name": "value",
          "type": "Number|Array<Number>",
          "description": "Input value may be an integer value or an array with a range",
          "default": "50"
        },
        {
          "name": "min",
          "type": "Number",
          "description": "The minimum permitted value",
          "default": 0
        },
        {
          "name": "max",
          "type": "Number",
          "description": "The maximum permitted value",
          "default": 0
        },
        {
          "name": "step",
          "type": "Number",
          "description": "The stepping interval, used both for user interface and validation purposes",
          "default": 0
        }
      ],
      "events": [],
      "slots": [],
      "cssProperties": [
        {
          "name": "--input-slider-range-color-primary",
          "description": "Main Color",
          "default": "#3f51b5"
        },
        {
          "name": "--input-slider-range-color-track",
          "description": "Track Color",
          "default": "#eee"
        },
        {
          "name": "--input-slider-range-color-fill",
          "description": "Track Fill Color",
          "default": "var(--input-slider-range-color-primary)"
        },
        {
          "name": "--input-slider-range-color-thumb",
          "description": "Thumb Color",
          "default": "var(--input-slider-range-color-primary)"
        },
        {
          "name": "--input-slider-range-color-outline",
          "description": "Outline Color",
          "default": "rgb(159 168 218 / 50%)"
        },
        {
          "name": "--input-slider-range-track-height",
          "description": "Track Height",
          "default": "4px"
        },
        {
          "name": "--input-slider-range-thumb-size",
          "description": "Thumb Size",
          "default": "18px"
        }
      ]
    }
  ]
}
